<!DOCTYPE html>
<html>
  <head>
    <title>Virtuali</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= stylesheet_link_tag "fonts", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= favicon_link_tag "favicon.ico"%>
<%#= javascript_include_tag :xdr_transport %>
    <%= javascript_include_tag "https://js.stripe.com/v1/" %>
    <%#= tag :meta, :name => "stripe-key", :content => STRIPE_PUBLIC_KEY %>
    <%= csrf_meta_tags %>
<%#*<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>%>
<%#*<meta http-equiv="X-UA-Compatible" content="IE=EmulateIE8" />%>
<%#= render 'layouts/navbar' %>
    <!--[if IE 8 ]>
<link href="http://www.seooffpage.com/ie/ie8.css" media="all" rel="stylesheet" type="text/css">
<![endif]-->
    <!--[if IE 7 ]>
    <link href="http://www.seooffpage.com/ie/ie7.css" media="all" rel="stylesheet" type="text/css">
    <![endif]-->
  </head>
  <body>
    <%= render 'layouts/navbar' %>
    <%if current_user%>

      <div id="wrap" style="margin-top: 16%;">
      <%else%>
        <div id="wrap">
        <%end%>

        <div class ="container " >
          <%= render 'shared/flash_messages' %>

<%#=yield :search%>
          <%if current_user%>

            <%=yield :side_bar%>
            <div id="jquery-container">
            <%else%>
              <div>
              <%end%>
              <%= yield %>
            </div>
            <%= yield :scripts %>

            <% if current_user %>
              <%= render 'feedbacks/feedback_form' %>
              <div id="feedback">
                <%= link_to image_tag('fdback.png'), "#example", "data-toggle" => "modal"%>
              </div>

            <%end%>
          </div>
          <div id="push"></div>
        </div>
      </div>
      <%= render 'layouts/footer' %>
  </body>
</html>
<style>
  #jquery-container{

    margin-left:25%;
    min-height: 700px;
    width: 925px;
    overflow: auto;
    margin-top:5%;

  }
  .nav [class^="icon-"], .nav [class*=" icon-"] {
    line-height: 0.9em;
  }

</style>