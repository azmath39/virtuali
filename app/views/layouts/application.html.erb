<!DOCTYPE html>
<html>
<head>
  <title>VirtualTour</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag "https://js.stripe.com/v1/" %>
  <%= tag :meta, :name => "stripe-key", :content => STRIPE_PUBLIC_KEY %>
  <%= csrf_meta_tags %>
 </head>
<body>
  <%= render 'layouts/navbar' %>
  <div style="height: 4px; width:100%; background-color: #0e120f; position:fixed;"></div>
  <div class =" container" style="margin-top: 5px;height:auto;">
    <%= render 'shared/flash_messages' %>
    <%= yield %>
    <%= yield :scripts %>
    <%# if current_user %>
    <%#= render 'feedbacks/feedback_form' %>
    <%#*<div id="feedback">%>
      <%#= link_to image_tag('fdback.png'), "#example", "data-toggle" => "modal"%>
    <%#*</div>%>
    <%# end %>
  </div>
     <%= render 'layouts/footer' %>
 </body>
</html>
