<%= stylesheet_link_tag    "application", :media => "all" %>
<table class="table table-hover">
  <tr>
    <th>S No.</th>
    <th>User Name</th>
    <th>Email</th>
    <th>Feedback</th>
    <th>Description</th>
    <th>&nbsp;</th>
  </tr>
  <% @i =1 %>
  <% @feedbacks.each do |feedback| %>
  <tr class="<%= feedback.satisfaction_status == 'yes' ? "success" : "error" %>">
    <td><%= @i %></td>
    <td><%= feedback.user.name %></td>
    <td><a href="mailto:#"><%= feedback.user.email %></a></td>
    <td><%= feedback.satisfaction_status %></td>
    <td><%= feedback.content %></td>
    <td><%= link_to "delete", {:controller => 'feedbacks', :action => 'destroy', :id => feedback.id}, :method => :delete, :confirm => "Are you sure?" %> </td>
  </tr>
  <% @i+=1 %>
  <% end %>
</table>