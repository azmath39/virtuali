<div class="span6">
<% @tours.each do |tour| %>
<div class="span6 well" id = 'img_bg' style="margin: 0 30px 1px 0px;">
  <div>
    <%#= t.paintings %>
    <%= link_to(image_tag(tour.paintings.compact.first.image_url(:small)), tour) unless tour.paintings.empty?  %>
  <div style =" margin: 0 300px 0 0;float: right;">
    <%= tour.state %><br />
    <%= tour.bed_rooms %> Beds / <%= tour.bath_rooms %> Baths<br />
    &#36; <%= tour.price %><br />
    <%= link_to "More information....", tour %><br />
    <p>Virtual Tour Available</p>
  </div>
    <div style="float:right;margin: 10px 20px;">
      <%=link_to "view", tour_path(tour.id)%>&nbsp;|
      <%=link_to "edit", edit_tour_path(tour.id)%>&nbsp;|
      <%=link_to "delete", tour_path(tour.id)%>
    </div>
  </div>
</div>
<% end %>
</div>