<div class="span6" style="margin: 0 0 0 0px;">
<% @tours.each do |tour| %>
<div class="span6 well" id = 'img_bg' style="margin: 0px 0 1px -210px;">
  <div>
    <%= link_to(image_tag(tour.paintings.compact.first.image_url(:small)), tour) unless tour.paintings.empty?  %>
  <div style ="float: right; margin: 0 310px 0 0">
    <%= tour.state %><br />
    <%= tour.bed_rooms %> Beds / <%= tour.bath_rooms %> Baths<br />
    &#36; <%= tour.price %><br />
    <%= link_to "More information....", tour %><br />
    <p>Virtual Tour Available</p>
  </div>
  </div><br />
</div>
<% end %>
</div>